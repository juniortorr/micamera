          <a href="/contact"><button>Inquire</button></a>
        </div>
        <a class="hidden-btn" href="/contact"><button>Inquire</button></a>
      </div>

      <div class="package-detail-wrap fade-in">
        <div class="packageDetail">
          <h1>Just You</h1>
          <p class="subheading">Single Portrait/Headshot Session</p>
          <div class="txt">
            <p>-45 minute session</p>
            <p>-1 outfit</p>
            <p>-1 location</p>
            <p>-minimum of 15 high res photos</p>
            <p>-online gallery</p>
            <p class="price">$185</p>
          </div>
          <a href="/contact"><button>Inquire</button></a>
        </div>
        <a class="hidden-btn" href="/contact"><button>Inquire</button></a>
      </div>
    </div>

    <%- include('partials/footer') %> <%- include('partials/scripts') %>
